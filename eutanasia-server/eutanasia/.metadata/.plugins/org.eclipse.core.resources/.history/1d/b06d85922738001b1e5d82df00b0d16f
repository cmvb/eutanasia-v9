package com.eutanasia.eutanasia.enums;

public enum EPlantillaCorreo {
	INVITACION_EVENTO("invitacionEvento.html"), SOLICITUD_MATERIAL("solicitudMaterial.html"),
	DONACIONES("donaciones.html");

	private final String nombre;

	private EPlantillaCorreo(String nombre) {
		this.nombre = nombre;
	}

	public String getNombre() {
		return nombre;
	}

}
