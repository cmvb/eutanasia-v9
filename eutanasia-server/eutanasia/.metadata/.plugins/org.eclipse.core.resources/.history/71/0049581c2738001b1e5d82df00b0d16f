package com.eutanasia.eutanasia.enums;

public enum EPlantillaCorreo {
	COMENTARIO_RESPUESTA("comentarioRespuesta.html"), NUEVO_USUARIO("nuevoUsuario.html"),
	RESTAURAR_CLAVE("restaurarClave.html");

	private final String nombre;

	private EPlantillaCorreo(String nombre) {
		this.nombre = nombre;
	}

	public String getNombre() {
		return nombre;
	}

}
